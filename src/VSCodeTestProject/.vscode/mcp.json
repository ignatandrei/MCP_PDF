{
  "servers": {
    "MCP_Array_Any": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "../MCP_PDF/MCP_PDF.csproj"
      ],
      "env": {
        "logMCPFile": "D:\\mcp-pdf-.log",
      }
    },
     "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${workspaceFolder}",
        "${workspaceFolder}/output"
        
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}